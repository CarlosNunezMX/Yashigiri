{
  "name": "newflow",
  "module": "source/index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "^8.2.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start:bun": "bun ./source/index.ts",
